{
  "sourceRoot": "src",
  "compilerOptions": {
    "plugins": [
      {
        "name": "@nestjs/swagger/plugin",
        "options": {
          "classValidatorShim": true,
          "introspectComments": true
        }
      }
    ],
    "assets": ["**/*.yml", "**/*.json"],
    "deleteOutDir": true
  }
}
